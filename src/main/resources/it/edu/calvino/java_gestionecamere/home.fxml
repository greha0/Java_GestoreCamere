<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox fx:id="home" alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.edu.calvino.java_gestionecamere.ControllerHome">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <AnchorPane prefHeight="167.0" prefWidth="267.0">
        <Label fx:id="title" layoutX="7.0" layoutY="14.0" prefHeight="27.0" prefWidth="195.0" text="Home" textOverrun="CLIP">
          <font>
              <Font name="Helvetica Bold" size="23.0" />
          </font>
        </Label>
        <Button fx:id="reserveButton" layoutX="14.0" layoutY="53.0" mnemonicParsing="false" onAction="#reservePage" prefHeight="34.0" prefWidth="150.0" text="Prenota" />
        <Button fx:id="feedbackButton" layoutX="14.0" layoutY="106.0" mnemonicParsing="false" onAction="#setTitle" prefHeight="40.0" prefWidth="150.0" text="Recensioni" />
    </AnchorPane>
</VBox>
